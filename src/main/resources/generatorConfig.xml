<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<classPathEntry location="D:\Tools\mysql-connector-java-5.1.39.jar" />

	<context id="mysql" targetRuntime="MyBatis3" defaultModelType="flat">

		<!-- beginningDelimiter和endingDelimiter：指明数据库的用于标记数据库对象名的符号，比如ORACLE就是双引号，MYSQL默认是`反引号； -->
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />

		<!-- 注释 -->
		<commentGenerator>
			<!-- 是否取消注释 -->
			<property name="suppressAllComments" value="true" />
			<!-- 是否生成注释代时间戳 -->
			<property name="suppressDate" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://172.16.7.111:3306/wec_biz_counselor_leave" userId="admin"
			password="wisedu@2016">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.wisedu.cpdaily.hotguy.entity.mysql"
			targetProject="cpdaily-hotguy-tool/src/main/java">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="mybatis.mapper.counselor"
			targetProject="cpdaily-hotguy-tool/src/main/resources">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.wisedu.cpdaily.hotguy.mapper.counselor"
			targetProject="cpdaily-hotguy-tool/src/main/java">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>

		<table tableName="t_coun_leave_record" domainObjectName="LeaveRecord">
			<!--  <generatedKey column="id" sqlStatement="Mysql" identity="true" /> -->
			<!--  <columnOverride column="C_USER_ID" property="createUserId" /> -->
			<!--  <ignoreColumn column="alias"/> -->
		</table>

	</context>

</generatorConfiguration>
